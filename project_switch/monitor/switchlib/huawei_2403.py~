#!/usr/bin/env python3

import re, sys
import decorator

# function "get_oid" and "walk_oid" has been import dynamicly by class.py
@decorator.rrd_options(DS='memory_uility')
def memory():
    total = int(re.search('Gauge32: (.*)\n', get_oid('1.3.6.1.4.1.2011.6.1.2.1.1.2.65536')).group(1))
    free = int(re.search('Gauge32: (.*)\n', get_oid('1.3.6.1.4.1.2011.6.1.2.1.1.3.65536')).group(1))
